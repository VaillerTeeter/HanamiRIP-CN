{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "HanamiRIP CN",
  "version": "0.1.0",
  "identifier": "com.hanamirip.cn",
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "HanamiRIP CN",
        "width": 1360,
        "height": 820,
        "resizable": false,
        "maximizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "icon": ["icons/icon.svg"]
  }
}
